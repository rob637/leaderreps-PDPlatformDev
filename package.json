{
  "name": "leader-reps-pdp",
  "private": true,
  "version": "1.5.30",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "echo '❌ BLOCKED: Use deploy:dev or deploy:test to prevent env mixup' && exit 1",
    "_build:called-by-deploy-script-only": "vite build",
    "visualize": "VISUALIZE=true vite build",
    "build:debug": "vite build --debug",
    "preview": "vite preview --port 4173",
    "clean": "rimraf build",
    "rebuild": "echo '❌ BLOCKED: Use deploy:dev or deploy:test' && exit 1",
    "deploy:dev": "bash ./scripts/deploy.sh dev",
    "deploy:test": "bash ./scripts/deploy.sh test",
    "deploy:repup:dev": "bash ./scripts/deploy-repup.sh dev",
    "deploy:repup:test": "bash ./scripts/deploy-repup.sh test",
    "deploy": "echo '❌ Use deploy:dev or deploy:test instead' && exit 1",
    "deploy:quick": "echo '❌ Use deploy:dev or deploy:test instead' && exit 1",
    "sync:test": "./scripts/deploy/sync-dev-to-test.sh",
    "lint": "eslint .",
    "lint:ui": "bash ./scripts/ui-architecture-check.sh",
    "precommit": "npm run lint:ui && npm run lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "e2e:smoke": "playwright test smoke.spec.js",
    "e2e:auth": "playwright test auth.spec.js --project=unauthenticated",
    "e2e:admin": "playwright test admin.spec.js --project=admin",
    "e2e:daily": "playwright test daily-practice.spec.js",
    "e2e:report": "playwright show-report",
    "e2e:local": "E2E_ENV=local playwright test",
    "e2e:test": "E2E_ENV=test playwright test",
    "e2e:prod": "E2E_ENV=prod playwright test",
    "e2e:live": "node scripts/run-e2e-firestore.cjs",
    "e2e:suite:auth": "E2E_ENV=test playwright test --project=suite-auth",
    "e2e:suite:prep": "E2E_ENV=test playwright test --project=suite-prep",
    "e2e:suite:am": "E2E_ENV=test playwright test --project=suite-am",
    "e2e:suite:pm": "E2E_ENV=test playwright test --project=suite-pm",
    "e2e:suite:content": "E2E_ENV=test playwright test --project=suite-content",
    "e2e:suite:post": "E2E_ENV=test playwright test --project=suite-post",
    "e2e:suite:zones": "E2E_ENV=test playwright test --project=suite-zones",
    "e2e:suites:all": "E2E_ENV=test playwright test e2e-tests/suites/",
    "data:export": "node scripts/migrate-app-data.cjs export",
    "data:import": "node scripts/migrate-app-data.cjs import",
    "data:list": "node scripts/migrate-app-data.cjs list",
    "cleanup:dev-preview": "node scripts/cleanup-test-users.cjs dev --dry-run",
    "cleanup:dev-execute": "node scripts/cleanup-test-users.cjs dev --execute",
    "cleanup:test-preview": "node scripts/cleanup-test-users.cjs test --dry-run",
    "cleanup:test-execute": "node scripts/cleanup-test-users.cjs test --execute"
  },
  "engines": {
    "node": ">=20.x"
  },
  "dependencies": {
    "@google/genai": "^1.27.0",
    "@monaco-editor/react": "^4.7.0",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "firebase": "^10.14.1",
    "firebase-admin": "^13.6.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.395.0",
    "marked": "^13.0.3",
    "openai": "^6.9.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-live": "^4.1.8",
    "recharts": "^3.3.0",
    "rxjs": "^7.8.2",
    "tailwind-merge": "^3.4.0",
    "web-vitals": "^5.1.0",
    "win-ca": "^3.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.12",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^27.1.0",
    "netlify-cli": "^23.9.1",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.18",
    "vite": "^5.4.20",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.8"
  }
}
